    "version": "0.2.0",
    "configurations": [
        

        {
            "type": "pwa-chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "http://localhost:49476",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Debug Jest Tests",
            "type": "node",
            "request": "launch",
            "runtimeArgs": [
              "--inspect-brk",
              "C:/Users/MESH USER/Desktop/Test/Bacs/node_modules/jest/bin/jest.js",
              "--runInBand",
              "-c", "${workspaceFolder}/bacs/jest.config.js"
            ],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "port": 4200
          }
    ]
}